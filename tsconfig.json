{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "layout/AppMenu.jsx", "layout/AppSidebar.jsx", "layout/AppMenuitem.jsx", "layout/AppTopbar.jsx", "layout/AppFooter.jsx", "layout/context/layoutcontext.tsx", "layout/context/menucontext.tsx", "app/(dashboard)/layout.js", "app/(dashboard)/(others-pages)/user-create/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}